import Vue from 'vue';
import Renderer from '<%= rendererPath %>';

const cacheBust = '<%= buildId %>';
const designTokens = <%= designTokens %>;

export default {
  render(createElement) {
    return createElement(Renderer, {
      props: Object.assign(designTokens, {
        name: '<%= name %>',
        importPath: '<%= importPath %>',
      }),
    });
  },
};
